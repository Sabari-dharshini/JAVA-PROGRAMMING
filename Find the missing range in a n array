class Main{
    public static void main(String[]args){
        int []arr={11,12,16,23,24,51,63,67,70};
        for(int i=0;i<arr.length-1;i++){
             if (arr[i + 1] - arr[i] > 1) {
                System.out.print("[" + (arr[i] + 1) + " " + (arr[i + 1] - 1) + "] ");
            }
            
          
        }
        } 
}
[13 15] [17 22] [25 50] [52 62] [64 66] [68 69] 

