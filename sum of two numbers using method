import java.util.Scanner;
class Add{
    private int a,b,c;
    public void get(){
        Scanner sc = new Scanner(System.in);
        a=sc.nextInt();
        b=sc.nextInt();
    }
    public void sum(){
        c=a+b;
        
    }
    public void display(){
        System.out.println("Sum of two numbers = "+c);
    }
}
class Main{
    public static void main(String[]args){
        Add obj=new Add();
        obj.get();
        obj.sum();
        obj.display();
    }
}
