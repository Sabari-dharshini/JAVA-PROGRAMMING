import java .util.Scanner;
class Main{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int sum=0;
        int n=sc.nextInt();
        while(n>0){
            int digit=n%10;
            n/=10;
            if(digit%2==0)
            {
                sum+=digit;
            }
            System.out.println(sum);
        }
    }
}
