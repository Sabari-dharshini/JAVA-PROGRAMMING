class Main {
    public static void main(String[] args) {
        int x=10;
        int b=--x + --x + --x + --x;
        System.out.println(b);
        int c=x++ + ++x + ++x + x++ + ++x;
        System.out.println(c);
        int d=--x + x++ + ++x + --x + x++;
        System.out.println(d);
        
        
    }
}
