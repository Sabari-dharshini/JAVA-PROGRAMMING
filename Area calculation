import java.util.Scanner;

class Example {
    
   
    static double Area(int a) {
        return a * a;
    }

   
    static double Area(double r) {
        double pi = 3.14;
        return pi * r * r; 
    }

    
    static double Area(int l, int b) {
        return l * b;
    }
}

class Main {
    public static void main(String[] args) {
        Example e = new Example();
        
        
        double rect = e.Area(20, 40);
        System.out.println("The area of the rectangle is " + rect);
        
        
        double circ = e.Area(5.0);  
        System.out.println("The area of the circle is " + circ);
        
       
        double squ = e.Area(8); 
        System.out.println("The area of the square is " + squ);
    }
}
