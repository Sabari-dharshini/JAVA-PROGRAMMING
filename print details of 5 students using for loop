import java .util.Scanner;
class Main{
    String name;
    int regNo;
    
    public void get(){
        Scanner scan=new Scanner(System.in);
       
        name=scan.next();
        regNo=scan.nextInt();
        
    }
    public void display(){
        System.out.println("The name of student is "+name);
        System.out.println("the register number of a student is "+regNo);
    }
    public static void main(String[]args){
        for(int i=1;i<=5;i++){
        Main s1=new Main();
        System.out.println("Enter the details of student "+i);
      s1.get();
      
      s1.display();
       
        
       
    }
}
}

