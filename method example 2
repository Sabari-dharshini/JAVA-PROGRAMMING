import java.util.Scanner;
class Add{
    private int a,b,c;
    private void get(){
        Scanner sc = new Scanner(System.in);
        a=sc.nextInt();
        b=sc.nextInt();
    }
    public void sum(){
        c=a+b;
        
    }
    public void display(){
        get();
        sum();
        System.out.println("Sum of two numbers = "+c);
        
    }
}
class Main{


    public static void main(String[]args){
        Add obj=new Add();
        
        obj.sum();
        obj.display();
    }
}

